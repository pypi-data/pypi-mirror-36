#!python

from ohh import cli, Runner, OhhError

def main():
    args = vars(cli.parse())
    action, basedir = args.pop('action'), args.pop('basedir')
    runner = Runner(basedir)

    try:
        runner.run(action, **args)
    except Exception as e:
        print("error: " + str(e))


if __name__ == '__main__':
    main()
