#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'wspr-server==0.1.8','console_scripts','wspr-server'
__requires__ = 'wspr-server==0.1.8'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('wspr-server==0.1.8', 'console_scripts', 'wspr-server')()
    )
