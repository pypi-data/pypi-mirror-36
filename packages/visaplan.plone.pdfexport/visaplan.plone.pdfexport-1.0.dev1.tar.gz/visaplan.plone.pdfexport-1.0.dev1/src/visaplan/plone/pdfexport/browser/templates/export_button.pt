<tal:block tal:define="Bexport nocall:here/@@export;"
           metal:define-macro="buttons">
    <div class="col-md-12"
         tal:define="url Bexport/getURL;"
         tal:condition="Bexport/canExport">
        <div class="row" style="margin-bottom:10px;">
            <a href="export_view"
               tal:attributes="href string:${url}/export_view"
               class="btn btn-default" i18n:translate="">
                Export
            </a>
        </div>
    </div>
</tal:block>
