[MASTER]
load-plugins = pylint.extensions.check_elif

[MESSAGES CONTROL]
disable =
    bad-continuation,
    invalid-name,
    locally-disabled,
    missing-docstring,
    no-else-return,
    too-many-arguments,
    useless-object-inheritance,

[REPORTS]
reports = no
msg-template = {C}: {path}:{line}: {symbol} [{obj}] {msg}

[FORMAT]
max-line-length = 140
expected-line-ending-format = LF

