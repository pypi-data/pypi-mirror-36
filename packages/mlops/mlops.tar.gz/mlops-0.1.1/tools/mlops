#!/usr/bin/env python

import mlops

def _run(args):
  mlops.run(args.resource_name)

if __name__ == '__main__':
  import argparse
  p = argparse.ArgumentParser()
  sub = p.add_subparsers()
  run = sub.add_parser('run', help='see `run -h`')
  run.add_argument('resource_name')
  run.set_defaults(handler=_run)
  a = p.parse_args()
  if hasattr(a, 'handler'):
    a.handler(a)
  else:
    p.print_help()
