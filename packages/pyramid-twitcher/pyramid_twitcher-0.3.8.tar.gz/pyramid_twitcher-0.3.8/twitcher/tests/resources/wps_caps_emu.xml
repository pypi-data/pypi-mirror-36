      <ows:Metadata xlink:href="http://emu.readthedocs.io/en/latest/" xlink:title="User Guide" xlink:type="simple"/>
    </wps:Process>
    <wps:Process wps:processVersion="1.0">
      <ows:Identifier>sleep</ows:Identifier>
      <ows:Title>Sleep Process</ows:Title>
      <ows:Abstract>This process will sleep for a given delay or 10 seconds if not a valid value</ows:Abstract>
      <ows:Metadata xlink:title="Sleep" xlink:type="simple"/>
      <ows:Metadata xlink:title="Wait" xlink:type="simple"/>
      <ows:Metadata xlink:title="Delay" xlink:type="simple"/>
    </wps:Process>
