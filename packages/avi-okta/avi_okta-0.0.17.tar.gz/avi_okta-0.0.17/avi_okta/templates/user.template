{
  "profile": {
    {% if organization %}
    "organization": "{{organization}}",
    {% endif %}
    "firstName": "{{first_name}}",
    "lastName": "{{last_name}}",
    "email": "{{email}}",
    "login": "{{email}}"
  },
  {% if password %}
  "credentials": {
    "password": "{{ password }}"
  },
  {% endif %}
  "groupIds": [
  {% for id in group_ids %}
    "{{id}}"{% if not loop.last %},{% endif %}
  {% endfor %}
  ]
}