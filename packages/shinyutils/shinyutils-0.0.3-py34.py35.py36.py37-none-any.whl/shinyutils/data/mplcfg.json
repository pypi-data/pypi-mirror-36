{
    "backend": "pgf",
    "pgf.texsystem": "lualatex",
    "pgf.rcfonts": true,
    "text.color": "k",
    "text.usetex": true,
    "font.family": "serif",
    "font.serif": "Latin Modern Roman",
    "font.sans-serif": "Latin Modern Sans",
    "lines.linewidth": 1,
    "lines.color": "k",
    "xtick.direction": "in",
    "xtick.color": "k",
    "xtick.major.size": 2,
    "xtick.major.width": 1,
    "xtick.major.pad": 5,
    "ytick.direction": "in",
    "ytick.color": "k",
    "ytick.major.size": 2,
    "ytick.major.width": 1,
    "ytick.major.pad": 2,
    "axes.linewidth": 1,
    "axes.edgecolor": "k",
    "axes.labelcolor": "k",
    "axes.spines.right": false,
    "axes.spines.top": false,
    "legend.frameon": false,
    "legend.fancybox": false,
    "savefig.format": "pdf",
    "savefig.transparent": true
}
